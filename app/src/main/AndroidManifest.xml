<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.samarpitha.shara.kannadalanguage">

    <application
        android:allowBackup="true"
        android:icon="@drawable/kannada_images"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.samarpitha.shara.kannadalanguage.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.samarpitha.shara.kannadalanguage.NumberActivity" />
        <activity android:name="com.samarpitha.shara.kannadalanguage.PhrasesActivity" />
        <activity android:name="com.samarpitha.shara.kannadalanguage.FamilyActivity" />
        <activity android:name="com.samarpitha.shara.kannadalanguage.ColorsActivity" />
        <activity android:name="com.samarpitha.shara.kannadalanguage.Alphabets"></activity>
    </application>

</manifest>